{
  "extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
  "plugins": ["jest"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 12
  },
  "env": {
    "browser": true,
    "es2021": true,
    "jest/globals": true
  },
  "rules": {
    "newline-before-return": "error",
    "no-case-declarations": "off"
  }
}
